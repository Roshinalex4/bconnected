<style>
#alphabet_drop_down {
	width: 212px;
	overflow: auto;
	background: #7F7F7F;
	background: rgba(0, 0, 0, 0.5);
	list-style: none;
}
#alphabet_drop_down li {
	border-bottom: 1px solid white;
	cursor: default;
	float: left;
	font-weight: bold;
	font-size: 12px;
	padding: 8px 0 0 0;
	text-align: center;
	opacity: 1;
	height: 20px;
	width: 28px;
	margin: 1px;
	color: #CCC;
	background: white;
	background: -moz-linear-gradient(top, #FEFEFE 0%, #DEDEDE 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #FEFEFE), color-stop(100%, #DEDEDE));
}

</style>
<h2>Contacts List</h2>
<% @contacts.each do |contact|%>
    <%= contact.user_profile.display_name %><br />
<% end %><br />
--------------------------------------------------------------------
<h2>Fiter Contacts</h2>
<%= form_tag "/user_profiles/filter" do %>
<div id="searchdiv_col">
	<div class="searchcol2">
		<%= text_field :user, :first_name, :class => "searchcol2_textfield" %>
	</div>
	<%= submit_tag "", :class => "searchdiv_col_btn" %>
</div>
<% end %><br clear="all"/>
--------------------------------------------------------------------
<ul id="alphabet_drop_down">
  <% "A".upto("Z") do |l| -%>	    
     <li><%= link_to l, filter_user_profiles_path("user"=>{"first_name"=>l}), :method => :post %></li>	
  <% end %>
</ul>
