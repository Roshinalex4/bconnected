	<div id="connection_wrapper">
		<table border="0">
			<tr>
				<td id="filter_connection">
				<form action="" name="frmSearch" id="frmSearch" method="post">
					<input type="text" name="txtKeyword" id="txtKeyword" value="Filter Connections" onfocus="if(this.value=='Filter Connections'){this.value='';this.style.color='#000000';}" onblur="if(this.value==''){this.value='Filter Connections';this.style.color='#999';}"/>
				</form></td>
