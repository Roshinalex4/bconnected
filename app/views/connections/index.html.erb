				<td id="abc">
				<div class="alphabetsub" style="display: none;">
					<div class="alphabetsub_first"></div>
					<div class="alphabetsub_Content">
						<ul>
							<%= form_tag "", :id => "alpha", :method => "POST" do%>
							<% "A".upto("M") do |l| -%>
							<li>
								<%= link_to l, "#", :class => "alphabet", :id => l %>
							</li>
							<% end %>
							<% end %>
							<script>
								$('.alphabet').click(function () {
									var query = $(this).attr("id");
									$('#alpha').attr('action', "user_profiles/filter?first_name="+query);
									$('#alpha').submit();
								});
							</script>
						</ul>
						<div class="alphabetsub_Content_sub">
							#
						</div>
						<ul>
							<%= form_tag "", :id => "alpha2", :method => "POST" do%>
							<% "N".upto("Z") do |l| -%>
							<li>
								<%= link_to l, "#", :class => "alphabet1", :id => l %>
							</li>
							<% end %>
							<% end %>
							<script>
								$('.alphabet1').click(function () {
									var query = $(this).attr("id");
									$('#alpha2').attr('action', "user_profiles/filter?first_name="+query);
									$('#alpha2').submit();
								});
							</script>
						</ul>
					</div>
					<div class="alphabetsub_last"></div>
				</div></td>
			</tr>
		</table>
	</div>
