				<div id="searchdiv">
					<%= form_tag "/user_profiles/search" do %>
						<h6>Search for someone by name: </h6>
						<div id="searchdiv_outter">
							<div id="searchdiv_col">
								<div class="searchcol2">
									<p>
										First Name:
									</p>
									<%= text_field :user, :first_name, :class => "searchcol2_textfield" %>
								</div>
								<div class="searchcol2">
									<p>
										Last Name:
									</p>
									<%= text_field :user, :last_name, :class => "searchcol2_textfield" %>
								</div>
								<%= submit_tag "", :class => "searchdiv_col_btn" %>
							</div>
							<p class="paginationp">
							  <% "a".upto("z") do |l| -%>	    
							     <%= link_to l, filter_user_profiles_path("user"=>{"first_name"=>l}) %>	
							  <% end %>
							  <a href="#">More</a>
							</p>
						</div>
					<% end 	%>
